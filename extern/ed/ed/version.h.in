#pragma once

// The numeric version format is AAAAABBBBBCCCCCDDDE where:
//
// AAAAA - major version number
// BBBBB - minor version number
// CCCCC - bugfix version number
// DDD   - alpha / beta (DDD + 500) version number
// E     - final (0) / snapshot (1)
//
// When DDDE is not 0, 1 is subtracted from AAAAABBBBBCCCCC. For example:
//
// Version      AAAAABBBBBCCCCCDDDE
//
// 0.1.0        0000000001000000000
// 0.1.2        0000000001000020000
// 1.2.3        0000100002000030000
// 2.2.0-a.1    0000200001999990010
// 3.0.0-b.2    0000299999999995020
// 2.2.0-a.1.z  0000200001999990011
//
#define LIBED_VERSION       $libed.version.project_number$ULL
#define LIBED_VERSION_STR   "$libed.version.project$"
#define LIBED_VERSION_ID    "$libed.version.project_id$"
#define LIBED_VERSION_FULL  "$libed.version$"

#define LIBED_VERSION_MAJOR $libed.version.major$
#define LIBED_VERSION_MINOR $libed.version.minor$
#define LIBED_VERSION_PATCH $libed.version.patch$

#define LIBED_PRE_RELEASE   $libed.version.pre_release$

#define LIBED_SNAPSHOT_SN   $libed.version.snapshot_sn$ULL
#define LIBED_SNAPSHOT_ID   "$libed.version.snapshot_id$"
